{
    "name": "daniyousefifar/sabaidea-shortener",
    "description": "SabaIdea URL Shortener",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Yousefi Far",
            "email": "daniyousefifar@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "ext-pdo": "*",
        "cakephp/validation": "^4.3",
        "monolog/monolog": "^2.5",
        "php-di/php-di": "^6.4",
        "selective/validation": "^2.0",
        "slim/psr7": "^1.5",
        "slim/slim": "4.*",
        "tuupola/slim-jwt-auth": "^3.6"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Src\\": "src/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public"
    }
}
